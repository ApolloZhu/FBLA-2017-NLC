use_frameworks!

target 'FBLA' do
  platform :ios, '8.0'
  
  # Layout
  pod 'SnapKit'

  # Control
  pod 'SwiftyStarRatingView', :git => 'https://github.com/ApolloZhu/SwiftyStarRatingView.git'
  pod 'PKHUD'
  pod 'MaterialKit', :git => 'https://github.com/ApolloZhu/MaterialKit.git'

  # Util
  pod 'Kingfisher'
  pod 'SwiftyJSON'

  # Data
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'GoogleMaps'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'GoogleSignIn'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
end
